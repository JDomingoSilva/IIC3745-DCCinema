<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Cost:</strong>
    <%= product.cost %>
  </p>

  <p>
    <strong>Volum:</strong>
    <%= product.volum %>
  </p>

  <p>
    <strong>Weight:</strong>
    <%= product.weight %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= product.type %>
  </p>

</div>
